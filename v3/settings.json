{
  "workbench.settings.applyToAllProfiles": [
    "editor.tabSize"
  ],
  "chat.editor.wordWrap": "on",
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "files.hotExit": "off",
  "task.quickOpen.history": 0,
  "telemetry.feedback.enabled": false,
  "editor.fontFamily": "Consolas",
  "editor.tabSize": 2,
  "editor.rulers": [120],
  "editor.folding": true,
  "editor.cursorWidth": 2,
  "editor.lineHeight": 1.6,
  "editor.lineNumbers": "on",
  "editor.glyphMargin": true,
  "editor.fontLigatures": true,
  "editor.wordWrapColumn": 120,
  "editor.mouseWheelZoom": true,
  "editor.smoothScrolling": true,
  "editor.cursorBlinking": "smooth",
  "editor.wordWrap": "wordWrapColumn",
  "editor.inlineSuggest.enabled": false,
  "editor.tokenColorCustomizations": {},
  "editor.suggestOnTriggerCharacters": false,
  "editor.language.colorizedBracketPairs": [],
  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.stickyScroll.maxLineCount": 3,
  "git.confirmSync": false,
  "git.decorations.enabled": true,
  "git.autoRepositoryDetection": false,
  "git.openRepositoryInParentFolders": "never",
  "github.copilot.renameSuggestions.triggerAutomatically": false,
  "python.createEnvironment.trigger": "off",
  "terminal.integrated.cursorStyle": "block",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.confirmOnExit": "always",
  "terminal.integrated.defaultLocation": "editor",
  "terminal.integrated.cursorStyleInactive": "none",
  "workbench.editor.highlightModifiedTabs": true,
  "window.restoreWindows": "none",
  "window.title": "${dirty}${folderName}${separator}${activeEditorShort} - VSCode",
  "workbench.colorCustomizations": {
    "editor.background": "#262645",
    "editor.lineHighlightBorder":"#262645",
    "editorIndentGuide.activeBackground1":"#3f3f5c",
    "editorIndentGuide.background1":"#313151",
    "editorStickyScrollHover.background": "#4c4c7b8a",
    "editor.selectionBackground":"#31315a",
    "editor.wordHighlightBackground":"#4c4c7b8a",
    "editor.wordHighlightStrongBackground":"#4c4c7b8a",
    "tab.inactiveBackground": "#20203e",
    "tab.inactiveForeground":"#8484a7",
    "activityBar.background":"#20203e",
    "tab.border":"#20203e",
    "focusBorder":"#5d5d74",
    "editorLineNumber.foreground":"#5d5d74",
    "editorLineNumber.activeForeground": "#f7ef57",
    "menubar.selectionBackground":"#242443",
    "button.background":"#333357",
    "sideBar.foreground":"#8484a7",
    "sideBar.background": "#20203e",
    "sideBarTitle.background": "#20203e",
    "sideBarSectionHeader.background": "#20203e",
    "list.hoverBackground": "#2a2a4d",
    "list.activeSelectionBackground":"#2a2a4d",
    "list.inactiveSelectionBackground": "#2a2a4d",
    "menu.background": "#242443",
    "menu.border":"#333357",
    "menu.separatorBackground":"#2a2a4d",
    "menu.foreground":"#8484a7",
    "menu.selectionBackground":"#2a2a4d",
    "titleBar.activeBackground": "#20203e",
    "titleBar.activeForeground":"#8484a7",
    "titleBar.inactiveForeground": "#8484a7",
    "editorWidget.border":"#333357",
    "editorWidget.background":"#242443",
    "editorGroupHeader.tabsBackground": "#20203e",
    "statusBar.background": "#20203e",
    "statusBar.noFolderBackground":"#20203e",
    "input.background": "#333357",
    "dropdown.background": "#333357"
  },
  "window.zoomPerWindow": false,
  "workbench.reduceMotion": "on",
  "workbench.list.scrollByPage": true,
  "workbench.commandPalette.history": 10,
  "workbench.editor.scrollToSwitchTabs": true,
  "workbench.tree.stickyScrollMaxItemCount": 3,
  "workbench.editor.tabSizingFixedMinWidth": 80,
  "workbench.editor.tabSizingFixedMaxWidth": 120,
  "workbench.colorTheme": "Default Dark+",
  "workbench.editor.focusRecentEditorAfterClose": false,
  "workbench.editor.historyBasedLanguageDetection": false,
  "search.quickOpen.includeHistory": false,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/env": true,
    "**/venv": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/env/**": true,
    "**/venv/**": true,
    "env-*": true
  },
  "workbench.iconTheme": "symbols"
}